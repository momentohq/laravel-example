{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "Apache-2.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/momentohq/client-sdk-php"
        },
        {
            "type": "vcs",
            "url": "https://github.com/poppoerika/laravel-momento-cache"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.37",
        "momentohq/client-sdk-php": "dev-main",
        "poppoerika/laravel-momento-cache": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
